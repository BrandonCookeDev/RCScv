    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "React Server",
            "cwd": "${workspaceFolder}/react-server",
            "program": "${workspaceFolder}/react-server/express-server.js"
        },
        {
            "name": "Python Module",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/python",
            "module": "app.py"
        }
    ]
}